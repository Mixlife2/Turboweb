{
    "compilerOptions": {
      "target": "es5",         
      "jsx": "react",            
      "strict": true,                
      "esModuleInterop": true,     
      "skipLibCheck": true,           
      "forceConsistentCasingInFileNames": true, 
      "module": "esnext",                
      "moduleResolution": "node",        
      "resolveJsonModule": true,       
      "isolatedModules": true,           
      "noEmit": true                    
    },
    "include": [
      "src/**/*.ts",     
      "src/**/*.tsx"   ],
    "exclude": [
      "node_modules"        
    ]
  }
  